<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/notebook_graph"
    app:startDestination="@id/noteBookFragment">

    <fragment
        android:id="@+id/noteBookFragment"
        android:name="com.suji.ish.suji.fragment.NoteBookFragment"
        android:label="fragment_note_book"
        tools:layout="@layout/fragment_note_book" >
        <action
            android:id="@+id/action_noteBookFragment_to_bookPageFragment"
            app:destination="@id/bookPageFragment"
            app:enterAnim="@anim/fragment_in" />
    </fragment>
    <fragment
        android:id="@+id/bookPageFragment"
        android:name="com.suji.ish.suji.fragment.BookPageFragment"
        android:label="fragment_book_page"
        tools:layout="@layout/fragment_book_page" />
</navigation>